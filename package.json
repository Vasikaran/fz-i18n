{
  "name": "fz-i18n",
  "version": "1.0.3",
  "description": "i18n handling",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib coverage",
    "test": "jest --coverage --no-cache",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build":"npm run build:commonjs",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vimalceg/fz-i18n.git"
  },
  "keywords": [
    "react",
    "i18n",
    "i18n",
    "component"
  ],
  "author": "vimalesan.ceg@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vimalceg/fz-i18n/issues"
  },
  "homepage": "https://github.com/vimalceg/fz-i18n#readme",
  "jest": {
    "rootDir": ".",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/library/"
    ],
    "unmockedModulePathPatterns": [
      "__tests__",
      "node_modules",
      ".*"
    ],
    "testPathDirs": [
      "<rootDir>/__tests__/"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html",
      "json-summary",
      "text"
    ],
    "moduleFileExtensions": [
      "js"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "dependencies": {
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.10",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.5"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-jest": "^17.0.2",
    "babel-preset-react": "^6.5.0",
    "check-es3-syntax-cli": "^0.1.3",
    "cross-env": "^1.0.7",
    "jest": "^17.0.3",
    "rimraf": "^2.5.4"
  }
}
